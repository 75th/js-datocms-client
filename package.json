{
  "name": "datocms-client",
  "version": "0.1.0",
  "description": "DatoCMS API client",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src test",
    "lint:autocorrect": "eslint src test --fix",
    "watch:lint": "esw -w src test",
    "watch:test": "mocha --watch --opts mocha.opts test/**/*_test.js",
    "test": "mocha --opts mocha.opts test/**/*_test.js",
    "prepublish": "npm run lib && npm run dist",
    "lib": "babel src --out-dir lib",
    "dist": "NODE_ENV=production browserify src/index.js -s DatoCmsClient -t babelify -g envify -g uglifyify > dist/datocms-client.min.js"
  },
  "keywords": [
    "datocms"
  ],
  "author": "datocms",
  "contributors": [
    {
      "name": "Stefano Verna",
      "email": "s.verna@cantierecreativo.net"
    }
  ],
  "homepage": "https://github.com/datocms/js-datocms-client",
  "bugs": {
    "url": "https://github.com/datocms/js-datocms-client/issues"
  },
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "babel-cli": "6.9.0",
    "babel-eslint": "6.0.4",
    "babel-plugin-syntax-async-functions": "6.8.0",
    "babel-plugin-transform-regenerator": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "dirty-chai": "1.2.2",
    "envify": "3.4.0",
    "eslint": "2.11.1",
    "eslint-config-airbnb-base": "3.0.1",
    "eslint-plugin-import": "1.8.1",
    "eslint-watch": "2.1.11",
    "mocha": "2.5.3",
    "nock": "8.0.0",
    "uglifyify": "3.0.1"
  },
  "dependencies": {
    "babel-polyfill": "6.9.1",
    "imgix-core-js": "1.0.3",
    "isomorphic-fetch": "2.2.1",
    "pluralize": "2.0.0",
    "query-string": "4.2.0",
    "slugify": "0.1.1"
  }
}
