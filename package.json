{
  "name": "datocms-client",
  "version": "0.2.4",
  "description": "DatoCMS API client and CLI tool",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/datocms/js-datocms-client.git"
  },
  "scripts": {
    "lint:autocorrect": "eslint src test --fix",
    "test": "eslint src test && mocha --opts mocha.opts test/**/*_test.js",
    "prepublish": "rimraf lib dist && npm run lib && npm run dist",
    "dist": "ADD_POLYFILLS=1 NODE_ENV=production webpack --progress; NODE_ENV=production webpack --progress",
    "dev": "NODE_ENV=production webpack --progress --watch",
    "lib": "babel src --out-dir lib"
  },
  "keywords": [
    "datocms",
    "cms",
    "metalsmith",
    "hexo"
  ],
  "bin": {
    "dato": "./bin/dato.js"
  },
  "author": "DatoCMS <support@datocms.com>",
  "contributors": [
    {
      "name": "Matteo Manzo",
      "email": "m.manzo@datocms.com"
    },
    {
      "name": "Stefano Verna",
      "email": "s.verna@datocms.com"
    }
  ],
  "homepage": "https://github.com/datocms/js-datocms-client",
  "bugs": {
    "url": "https://github.com/datocms/js-datocms-client/issues"
  },
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-regenerator": "6.16.1",
    "babel-preset-es2015": "6.16.0",
    "chai": "3.5.0",
    "dirty-chai": "1.2.2",
    "eslint": "3.7.1",
    "eslint-config-airbnb-base": "8.0.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "2.0.0",
    "mocha": "3.1.0",
    "nock": "8.0.0",
    "nock-vcr-recorder": "0.1.5",
    "updeep": "1.0.0",
    "webpack": "1.13.2",
    "webpack-visualizer-plugin": "0.1.5"
  },
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "cli-spinner": "0.2.5",
    "denodeify": "1.2.1",
    "docopt": "0.6.2",
    "humps": "1.1.0",
    "js-yaml": "3.6.1",
    "mkdirp": "0.5.1",
    "node-fetch": "1.6.3",
    "pluralize": "3.0.0",
    "pretty-error": "2.0.1",
    "query-string": "4.2.3",
    "request": "2.75.0",
    "rimraf": "2.5.4",
    "speakingurl": "10.0.0",
    "tmp": "0.0.29",
    "toml-js": "0.0.8",
    "whatwg-fetch": "1.0.0"
  }
}
